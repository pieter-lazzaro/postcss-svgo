{
  "name": "postcss-svgo",
  "version": "2.0.3",
  "description": "Optimise inline SVG with PostCSS.",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "babel src --out-dir dist --ignore /__tests__/",
    "test-unformatted": "babel-tape-runner \"src/**/__tests__/*.js\"",
    "test": "npm run test-unformatted | tap-spec"
  },
  "files": [
    "LICENSE-MIT",
    "dist"
  ],
  "keywords": [
    "css",
    "minify",
    "optimise",
    "postcss",
    "postcss-plugin",
    "svg",
    "svgo"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.21",
    "babel-tape-runner": "^1.2.0",
    "jspm": "^0.16.10",
    "pleeease-filters": "^2.0.0",
    "tap-spec": "^4.1.0",
    "tape": "^4.2.0"
  },
  "homepage": "https://github.com/ben-eb/postcss-svgo",
  "author": {
    "name": "Ben Briggs",
    "email": "beneb.info@gmail.com",
    "url": "http://beneb.info"
  },
  "repository": "ben-eb/postcss-svgo",
  "dependencies": {
    "is-svg": "^1.1.1",
    "postcss": "^5.0.2",
    "postcss-value-parser": "^2.0.5",
    "svgo": "^0.5.6"
  },
  "registry": "jspm",
  "jspm": {
    "main": "index",
    "format": "es6",
    "directories": {
      "lib": "src"
    },
    "files": [
      "src"
    ],
    "dependencies": {
      "is-svg": "npm:is-svg@^1.1.1",
      "postcss": "npm:postcss@^5.0.2",
      "postcss-safe-parser": "npm:postcss-safe-parser@^1.0.1",
      "postcss-value-parser": "npm:postcss-value-parser@^2.0.5",
      "svgo": "github:pieter-lazzaro/svgo@master"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  }
}
